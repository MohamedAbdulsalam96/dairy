delivery_note) %}\n            <!--<tr>-->\n            <!--  <th id=\"total\" colspan=\"4\">Total :</th>-->\n            <!--  <td>{{del_note_total.total_supp_qty}}</td>-->\n            <!--  <td> {{ del_note_total.total_crate_qty }}</td>-->\n            <!--  <td>{{ del_note_total.total_free_qty }}</td>-->\n            <!--</tr>-->\n            {% for itm in del_note_total %}\n                    <tr>\n                    \n                    <th id=\"total\" colspan=\"5\">Total :</th>\n                      <td>{{itm.total_supp_qty}}</td>\n                      <td>{{ itm.total_free_qty }}</td>\n                      <td> {{ itm.total_crate_qty }}</td>\n                      <td></td>\n                    </tr>\n                     \n            {% endfor %}\n       </tfoot>\n\t\t\n</table>\n<hr>\n\n {% endfor %}\n \n\n<!--<TABLE border=\"1\">-->\n<!--<CAPTION><EM>A test table with merged cells</EM></CAPTION>-->\n<!--<TR><TH rowspan=\"2\"><TH colspan=\"2\">Average-->\n<!--    <TH rowspan=\"2\">Red<BR>eyes-->\n<!--<TR><TH>height<TH>weight-->\n<!--<TR><TH>Males<TD>1.9<TD>0.003<TD>40%-->\n<!--<TR><TH>Females<TD>1.7<TD>0.002<TD>43%-->\n<!--</TABLE>-->\n\n\n",
