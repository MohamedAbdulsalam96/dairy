 "html": "<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Arial;\n\t\tline-height: 100%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 8.3in;\n\t\t\tpadding: 0.20in;\n\t\t\tmin-height: 11.7in;\n\t\t}\n\t}\n</style>\n\n{% if letter_head %}\n    {{ letter_head }}\n{% endif %}\n<div class=\"text-center\">\n         {% set warehouse_address = warehouse_address(doc.warehouse) %}\n      {%- for row in warehouse_address -%}\n        <p>{{row}}</p>\n    {% endfor%}\n\t<u><b style = \"font-size: 15px\">{{ _(\"Gate Pass Details\") }}</b></u>\n</div>\n<hr>\n    <table style=\"width:100%\">\n        <tr>\n            <td><b>{{ _(\"Transporter\") }}:</b> {{ doc.transporter_name }}</td>\n            <td style = \"text-align: right;\"><b>{{ _(\"Gate Pass No\") }}:</b> {{ doc.name }}</td>\n           \n           \n        </tr>\n         <tr>\n            \n            <td><b>{{ _(\"Vehicle\") }}:</b> {{ doc.vehicle }}</td>\n            <td style = \"text-align: right;\"><b>{{ _(\"Shift\") }}:</b> {{ doc.shift }}</td>\n        </tr>\n        <tr>\n            \n            <td><b>{{ _(\"Vehicle No\") }}:</b>  _____________</td>\n \n           <td style = \"text-align: right;\"><b>{{ _(\"Date\") }}:</b>{{ frappe.format(doc.date, {'fieldtype': 'Date'}) }}</td>\n        </tr>\n        \n    </table>\n\n\t {% set dist_del_note= get_jinja_data_del_note(doc) %}\n\t       \n\t       {% for del_note in dist_del_note %}\n\t        {% set del_note_detail = del_note_details(del_note.delivery_note) %}\n                   <table style=\"width:100%\"  border=\"1\">\n                \t<thead>\n            \t    \t{% for itm in del_note_detail %}\n                \t    <tr>\n                \t\t\t<td colspan=\"2\">Challan No: {{itm.name}}</td>\n                            <td colspan=\"4\">Party Name: {{itm.customer_name}}</td>\n                            <td colspan=\"4\"> {{itm.route}}</td>\n                \t\t</tr>\n                \t    {% endfor %}\n                \t\t<tr>\n                \t\t    <th>SL.No</th>\n                \t\t\t<th>Item</th>\n                            <th>Crate Type</th>\n                            <th>Batch Code</th>\n                            <th>Unit</th>\n                            <th>Supp-Qty</th>\n                            <th>Free</th>\n                            <th>Crate</th>\n                            <th>Crate Return</th>\n                \t\t\t<th>Signature</th>\n                \t\t</tr>\n                \t\t\t\n                \t</thead>\n                \t\t<tbody>\n               {% set del_note_item = get_jinja_data_del_note_item(del_note.delivery_note) %}\n                     \n                    {% for itm in del_note_item %}\n                    <tr>\n                    <td>{{ loop.index }} </td>\n                    <td>{{itm.item_name}}</td>\n                    <td>{{itm.crate_type}}</td>\n                    <td>{{itm.batch_no}}</td>\n                    <td>{{itm.stock_uom}}</td>\n                    <td style = \"text-align: right;\">{{itm.stock_qty}}</td>\n                    <td style = \"text-align: right;\">{{itm.free_qty}}</td>\n                    <td style = \"text-align: right;\">{{itm.outgoing_count}}</td>\n                    <td style = \"text-align: right;\">{{itm.incoming_count}}</td>\n                    <td> </td>\n                    </tr>\n                     {% endfor %}\n\t\n\t\t</tbody>\n\t\t<tfoot>\n\t\t    {% set del_note_total = del_note_total(del_note.delivery_note) %}\n\n            {% for itm in del_note_total %}\n                    <tr>\n                    \n                    <th id=\"total\" colspan=\"5\">Total :</th>\n                      <td style = \"text-align: right;\">{{itm.stock_qty}}</td>\n                      <td style = \"text-align: right;\">{{ itm.fre_qty }}</td>\n                      <td style = \"text-align: right;\">